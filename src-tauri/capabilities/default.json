{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main",
    "install-wizard"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "dialog:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://skills.sh/*"
        }
      ]
    },
    "updater:default",
    "process:default",
    "os:default"
  ]
}
